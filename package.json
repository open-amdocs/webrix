{
  "name": "webrix",
  "description": "Powerful building blocks for React-based web applications",
  "keywords": [
    "react",
    "ui",
    "library",
    "components",
    "web"
  ],
  "version": "0.0.0-development",
  "bugs": "https://github.com/open-amdocs/webrix/issues",
  "homepage": "https://webrix.amdocs.com",
  "license": "Apache-2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "module": "index.es.js",
  "exports": {
    "./": {
      "require": "./index.js",
      "import": "./index.es.js",
      "default": "./index.js"
    },
    "./hooks": {
      "require": "./hooks/index.js",
      "import": "./hooks/index.es.js",
      "default": "./hooks/index.js"
    },
    "./hooks/*": {
      "require": "./hooks/*/index.js",
      "import": "./hooks/*/index.es.js",
      "default": "./hooks/*/index.js"
    },
    "./components": {
      "require": "./components/index.js",
      "import": "./components/index.es.js",
      "default": "./components/index.js"
    },
    "./components/*": {
      "require": "./components/*/index.js",
      "import": "./components/*/index.es.js",
      "default": "./components/*/index.js"
    },
    "./tools": {
      "require": "./tools/index.js",
      "import": "./tools/index.es.js",
      "default": "./tools/index.js"
    },
    "./tools/*": {
      "require": "./tools/*/index.js",
      "import": "./tools/*/index.es.js",
      "default": "./tools/*/index.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "7.16.7",
    "classnames": "2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.16.8",
    "@babel/core": "7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-decorators": "7.16.7",
    "@babel/plugin-proposal-export-default-from": "7.16.7",
    "@babel/plugin-proposal-function-bind": "7.16.7",
    "@babel/plugin-proposal-optional-chaining": "7.16.7",
    "@babel/plugin-transform-runtime": "7.16.10",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/register": "7.16.9",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.6",
    "babel-loader": "8.2.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-rewire": "1.2.0",
    "enzyme": "3.11.0",
    "eslint": "8.8.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-notice": "0.9.10",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest": "27.4.7",
    "postcss": "8.4.5",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.66.1",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "sass": "1.49.0",
    "semantic-release": "19.0.3",
    "sinon": "13.0.0",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "prop-types": ">=15.6.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/open-amdocs/webrix.git"
  },
  "scripts": {
    "build:dev": "rollup --config rollup/rollup.dev.js --watch",
    "build:prod": "rollup --config rollup/rollup.prod.js && npx tsc",
    "test": "jest .test.js",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint --ext=js --ext=jsx src/",
    "cleanup": "node ./cleanup.js"
  }
}
