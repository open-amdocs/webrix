{
  "name": "webrix",
  "description": "Powerful building blocks for React-based web applications",
  "keywords": [
    "react",
    "ui",
    "library",
    "components",
    "web"
  ],
  "version": "0.0.0-development",
  "bugs": "https://github.com/open-amdocs/webrix/issues",
  "homepage": "https://webrix.amdocs.com",
  "license": "Apache-2.0",
  "main": "cjs/index.js",
  "types": "typs/index.d.ts",
  "module": "es/index.js",
  "exports": {
    "./": {
      "require": "./cjs/index.js",
      "import": "./es/index.js",
      "default": "./cjs/index.js"
    },
    "./hooks": {
      "require": "./cjs/hooks/index.js",
      "import": "./es/hooks/index.js",
      "default": "./cjs/hooks/index.js"
    },
    "./hooks/*": {
      "require": "./cjs/hooks/*/index.js",
      "import": "./es/hooks/*/index.js",
      "default": "./cjs/hooks/*/index.js"
    },
    "./components": {
      "require": "./cjs/components/index.js",
      "import": "./es/components/index.js",
      "default": "./cjs/components/index.js"
    },
    "./components/*": {
      "require": "./cjs/components/*/index.js",
      "import": "./es/components/*/index.js",
      "default": "./cjs/components/*/index.js"
    },
    "./tools": {
      "require": "./cjs/tools/index.js",
      "import": "./es/tools/index.js",
      "default": "./cjs/tools/index.js"
    },
    "./tools/*": {
      "require": "./cjs/tools/*/index.js",
      "import": "./es/tools/*/index.js",
      "default": "./cjs/tools/*/index.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "7.14.8",
    "classnames": "2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-decorators": "7.14.5",
    "@babel/plugin-proposal-export-default-from": "7.14.5",
    "@babel/plugin-proposal-function-bind": "7.14.5",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-react": "7.14.5",
    "@babel/register": "7.14.5",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.2",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-rewire": "1.2.0",
    "enzyme": "3.11.0",
    "eslint": "7.31.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-notice": "0.9.10",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "27.0.6",
    "postcss": "8.3.6",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rollup": "2.54.0",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-terser": "7.0.2",
    "sass": "1.36.0",
    "semantic-release": "17.4.4",
    "sinon": "11.1.1",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "prop-types": ">=15.6.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/open-amdocs/webrix.git"
  },
  "scripts": {
    "build:dev": "rollup --config rollup/rollup.dev.js --watch",
    "build:prod": "rollup --config rollup/rollup.prod.js && npx tsc",
    "test": "jest .test.js",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint --ext=js --ext=jsx src/",
    "cleanup": "node ./cleanup.js"
  }
}
